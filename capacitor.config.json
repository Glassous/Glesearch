{
  "appId": "com.glassous.glesearch",
  "appName": "Glesearch",
  "webDir": "dist",
  "plugins": {
    "StatusBar": {
      "style": "LIGHT",
      "backgroundColor": "#00000000",
      "overlaysWebView": true
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    }
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true,
    "useLegacyBridge": false
  }
}
